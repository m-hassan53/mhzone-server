{
  "name": "mhzone-server",
  "version": "1.0.0",
  "description": "backend server to support angular 7 mhzone front end",
  "main": "dist/index.js",
  "scripts": {
    "format": "eslint src/**/*.ts src/*.ts --fix",
    "postinstall": "npm run format && npm run server-build",
    "nodemon-local": "nodemon --watch src --ext ts --exec npm start -V",
    "nodemon-cast": "nodemon --watch src --ext ts --exec npm start -V 10.0.0.237",
    "start": "node .",
    "server-build": "tsc --p ./",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "backend",
    "mhzone",
    "server"
  ],
  "author": "Muhammad Hassan",
  "license": "ISC",
  "dependencies": {
    "@sendgrid/mail": "^6.3.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "express-session": "^1.15.6",
    "mongoose": "^5.4.3",
    "morgan": "~1.9.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/express-session": "^1.15.11",
    "@types/mongoose": "^5.3.7",
    "@types/morgan": "^1.7.35",
    "@types/node": "^12.11.1",
    "@types/passport": "^1.0.0",
    "@types/passport-local": "^1.0.33",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "concurrently": "^6.2.1",
    "dotenv": "^10.0.0",
    "eslint": "^8.0.0",
    "nodemon": "^1.18.9",
    "typescript": "~4.3.5"
  }
}
